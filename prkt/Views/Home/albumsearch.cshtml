<div id="result">
    <h2>@ViewBag.response.title </h2><img src="@ViewBag.response.cover_big">
    <br> <h5>Жанры:@foreach (var item in ViewBag.genres)
    {
        @item.name 
    }
    </h5>
    <br>
    <table class="table table-bordered">
        <thead>
        <th>Имя артиста</th>
        <th>Название песни</th>
        <th>Рейтинг песни</th>
        <th>Превью песни</th>
        </thead>
    @foreach(var item in ViewBag.tracks)
    {
        <tr>
        <td>@item.artist.name</td>
        <td><a href="item.link">@item.title</a></td>
        <td>@item.rank</td>
        <td><audio controls><source src="@item.preview" type="audio/mp4"></audio></td>
        </tr>
    }
    </table>
</div>

